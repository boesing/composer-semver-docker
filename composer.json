{
    "name": "boesing/composer-semver-docker",
    "type": "project",
    "description": "Docker image to have a simple binary to test a specific version against a constraint",
    "license": "BSD-3-Clause",
    "require": {
        "php": "~8.1.0",
        "composer-runtime-api": "^2.0",
        "composer/semver": "^3.3",
        "symfony/console": "^5.4",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "humbug/box": "^3.16",
        "vimeo/psalm": "^4.23"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Boesing\\ComposerSemverDocker\\": "src/"
        }
    }
}
